/* components/home-navigator/index.wxss */
.home-navigator {
  display: flex;
  width: 750rpx;
  position: fixed;
  align-items: center;
  box-sizing: border-box;
  top: 0;
  padding: 0 20rpx;
  flex-wrap: nowrap;
  white-space: nowrap;
  background: #fff;
  border-bottom: 1rpx solid #e9e9e9;
  z-index: 1;
}

.home-navigator .icon {
  width: 80rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
}

.home-navigator .icon image {
  width: 50rpx;
  height: 50rpx;
}

.home-navigator scroll-view {
  width: 650rpx;
}

.home-navigator scroll-view .list{
  display: flex;
  height: 100rpx;
  align-items: center;
}